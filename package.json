{
  "name": "scrivito-portal-app",
  "description": "Scrivito Portal App",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=22.0"
  },
  "type": "module",
  "scripts": {
    "build": "vite build && rm -f dist/assets/*.js.map",
    "types": "tsc",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0 'src/**/*.ts' 'src/**/*.tsx' && knip",
    "lighthouse": "SCRIVITO_ORIGIN=http://localhost:8080 PRERENDER_OBJ_ID=c2a0aab78be05a4e npm run prerender && npm run prerender-precompress && for run in $(seq 6); do lhci autorun && break; done",
    "prerender": "vite build && vite-node --options.deps.external='/scrivito/' src/prerenderContent.ts",
    "prerender-precompress": "precompress --include '(**).(css|html|js)' --types br dist-ssr",
    "prerender-preview": "ws",
    "prettier": "prettier --check .",
    "start": "vite"
  },
  "dependencies": {
    "@dr.pogodin/react-helmet": "^3.0.5",
    "@honeybadger-io/js": "^6.12.3",
    "@justrelate/icon-picker": "^1.0.1",
    "@justrelate/slugify": "^1.0.1",
    "@radix-ui/react-slider": "^1.3.6",
    "history": "^5.3.0",
    "lodash-es": "^4.17.23",
    "pretty-bytes": "^7.1.0",
    "react": "^19.2.3",
    "react-bootstrap": "^3.0.0-beta.5",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.8",
    "react-toastify": "^11.0.5",
    "scrivito": "^1.58.0",
    "scrivito-neoletter-form-widgets": "^0.0.42",
    "scrivito-pisasales-questionnaire-builder": "^1.1.1",
    "scroll-to-fragment": "^1.1.0"
  },
  "devDependencies": {
    "@arabasta/eslint-plugin-react": "^1.0.0",
    "@cloudflare/workers-types": "^4.20260124.0",
    "@eslint/compat": "^2.0.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@honeybadger-io/rollup-plugin": "^5.10.1",
    "@lhci/cli": "^0.15.1",
    "@types/jsontoxml": "^1.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "bootstrap-icons": "^1.13.1",
    "content-security-policy-builder": "^2.3.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "hash-wasm": "^4.12.0",
    "jsontoxml": "^1.0.1",
    "knip": "^5.82.1",
    "local-web-server": "^5.4.0",
    "precompress": "^13.0.3",
    "prettier": "^3.8.1",
    "sass": "^1.78.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-node": "^5.3.0"
  },
  "overrides": {
    "@lhci/cli": {
      "cookie": "^0.7.0",
      "rimraf": "4",
      "tmp": "^0.2.4"
    }
  }
